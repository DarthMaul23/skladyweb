    "superagent": "^8.1.2",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.0",
    "naive-ui": "^2.37.3",
    "sass": "^1.49.8",
    "vite": "^2.8.0"
  }
}
